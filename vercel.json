{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/build/$1",
      "headers": {
        "Cache-Control": "s-maxage=31536000, stale-while-revalidate"
      }
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://yourapp-production.up.railway.app"
  }
}
